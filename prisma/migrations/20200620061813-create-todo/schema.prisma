// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Todo {
  id      Int      @default(autoincrement()) @id
  title   String   @unique
  done    Boolean  @default(false)
}
